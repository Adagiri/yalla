{
  "name": "propatize",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "watch": "nodemon index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && npm run copy:gql",
    "copy:gql": "node scripts/copy-gql.js",
    "lint": "eslint 'src/**/*.{ts,js}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,json,md}'",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "@aws-sdk/client-location": "^3.817.0",
    "@aws-sdk/client-s3": "^3.777.0",
    "@aws-sdk/client-ses": "^3.713.0",
    "@aws-sdk/s3-request-presigner": "^3.777.0",
    "@graphql-tools/load-files": "^7.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^8.18.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase-admin": "^13.4.0",
    "graphql": "^16.9.0",
    "graphql-date-scalars": "^0.2.0",
    "graphql-redis-subscriptions": "^2.7.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.24.0",
    "graphql-subscriptions": "^3.0.0",
    "graphql-tag": "^2.12.6",
    "graphql-ws": "^6.0.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.6",
    "mongoose": "^8.8.4",
    "shelljs": "^0.8.5",
    "socket.io": "^4.8.1",
    "twilio": "^5.4.0",
    "uuid": "^11.0.3",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/graphql-resolvers": "^0.3.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
